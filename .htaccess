RewriteEngine On
RewriteRule    ^home-page?$   index.php
RewriteRule    ^Login?$   login.php
RewriteRule    ^Register?$   register.php
RewriteRule    ^Cart-([0-9a-zA-Z]+)-([0-9a-zA-Z]+)  shopping-cart.php?product_id=$1&key=$2
RewriteRule    ^Cart-([0-9a-zA-Z]+)  shopping-cart.php?key=$1
RewriteRule    ^Product-Details-([0-9a-zA-Z]+)  single-product.php?product_sno=$1